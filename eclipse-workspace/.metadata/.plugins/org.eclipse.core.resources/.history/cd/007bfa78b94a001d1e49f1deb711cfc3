package Practice;
import java.util.Scanner;

public class IncOperator {

	public static void main(String[] args) {
		
		int grade;
		Scanner input = new Scanner(System.in);
		
		System.out.print("성적 입력: ");
		grade = input.nextInt();
		
		if(grade>=90)
		{
			System.out.println("학점 A");
		}
		else if(grade>=80)
		{
			System.out.println("학점 B");
		}
	}
}
