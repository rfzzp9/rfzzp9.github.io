package Practice;
import java.util.*;

public class IncOperator {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("정수:");
		int num = sc.nextInt();
		int gop=1;
		
		for(int i=1; i<=num; i++)
		{
			gop=gop*i;
		}
		System.out.println(num+"!은 "+gop+"입니다.");
	}
}
