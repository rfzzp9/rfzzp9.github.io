package Practice;
import java.util.*;

public class IncOperator {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("정수:");
		int num = sc.nextInt();
		int sum=1;
		
		for(int i=1; i<num; i++)
		{
			sum*=i;
		}
		System.out.println(num+"!은 "+sum+"입니다.");
	}
}
