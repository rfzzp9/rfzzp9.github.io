package exam2;

import java.util.Random;

class Circle
{
	int radius;
	public Circle(int radius)
	{
		this.radius = radius;
	}
	public boolean equals(Circle c1)
	{
		if(radius==c1.radius)
		{
			return true;
		}
		else
		{
			return false;
		}
	}
}
public class CircleTest {
	public static void main(String[] args) {
		Random random = new Random();
		for(int i=0; i<10; i++)
		{
			System.out.println(random.nextInt());
		}
	}
}
