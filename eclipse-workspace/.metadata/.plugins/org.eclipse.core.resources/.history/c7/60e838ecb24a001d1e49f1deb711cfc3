package Practice;

public class IncOperator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x=1, y=1;
		
		int a=x++;
		int b=++y;
		
		System.out.println("a="+a+", b="+b); //a=1, b=2
		
		int c = 100, d = 200;
		
		c+=10;
		d/=10;
		
		System.out.println("c="+c+", d="+d); //c=110, d=10;
	}

}
